.btn-area {
  width: 100%;
  height: 6%;
  background-color: #f6f6f6;
  display: flex;
  flex-direction: row;
  align-items: center;
}
.navigator-item {
  width: 33.3%;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-top: 4rpx;
}
.navigator-item image {
  display: block;
  width: 45rpx;
  height: 40rpx;
  margin-top: 4rpx;
}
.navigator-item text {
  font-size: 20rpx;
  color: #696969;
}